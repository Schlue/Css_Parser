   <install as="Sabberworm/CSS/Property/AtRule.php" name="lib/Sabberworm/CSS/Property/AtRule.php" />
   <install as="Sabberworm/CSS/Property/Charset.php" name="lib/Sabberworm/CSS/Property/Charset.php" />
   <install as="Sabberworm/CSS/Property/CSSNamespace.php" name="lib/Sabberworm/CSS/Property/CSSNamespace.php" />
   <install as="Sabberworm/CSS/Property/Import.php" name="lib/Sabberworm/CSS/Property/Import.php" />
   <install as="Sabberworm/CSS/Property/Selector.php" name="lib/Sabberworm/CSS/Property/Selector.php" />
   <install as="Sabberworm/CSS/Rule/Rule.php" name="lib/Sabberworm/CSS/Rule/Rule.php" />
   <install as="Sabberworm/CSS/RuleSet/AtRuleSet.php" name="lib/Sabberworm/CSS/RuleSet/AtRuleSet.php" />
   <install as="Sabberworm/CSS/RuleSet/DeclarationBlock.php" name="lib/Sabberworm/CSS/RuleSet/DeclarationBlock.php" />
   <install as="Sabberworm/CSS/RuleSet/RuleSet.php" name="lib/Sabberworm/CSS/RuleSet/RuleSet.php" />
   <install as="Sabberworm/CSS/Value/Color.php" name="lib/Sabberworm/CSS/Value/Color.php" />
   <install as="Sabberworm/CSS/Value/CSSFunction.php" name="lib/Sabberworm/CSS/Value/CSSFunction.php" />
   <install as="Sabberworm/CSS/Value/PrimitiveValue.php" name="lib/Sabberworm/CSS/Value/PrimitiveValue.php" />
   <install as="Sabberworm/CSS/Value/RuleValueList.php" name="lib/Sabberworm/CSS/Value/RuleValueList.php" />
   <install as="Sabberworm/CSS/Value/Size.php" name="lib/Sabberworm/CSS/Value/Size.php" />
   <install as="Sabberworm/CSS/Value/String.php" name="lib/Sabberworm/CSS/Value/String.php" />
   <install as="Sabberworm/CSS/Value/URL.php" name="lib/Sabberworm/CSS/Value/URL.php" />
   <install as="Sabberworm/CSS/Value/Value.php" name="lib/Sabberworm/CSS/Value/Value.php" />
   <install as="Sabberworm/CSS/Value/ValueList.php" name="lib/Sabberworm/CSS/Value/ValueList.php" />
  </filelist>
 </phprelease>
